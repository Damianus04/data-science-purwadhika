  {
   "cell_type": "code",
   "execution_count": 39,
   "metadata": {},
   "outputs": [],
   "source": [
    "jkt = r'jakarta_by_kelurahan_geo.geojson'\n",
    "\n",
    "# Set latitude, longitude\n",
    "lat= -6.175110\n",
    "long= 106.865036\n",
    "jkt_loc = [lat, long]\n",
    "jkt_map = folium.Map(location=jkt_loc, zoom_start=12)"
   ]
  },
  {
